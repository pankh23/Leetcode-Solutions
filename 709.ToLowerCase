class Solution {
    public String toLowerCase(String s) {
        
        StringBuilder res=new StringBuilder();

        for(int i=0;i<s.length();i++){

            char ch=s.charAt(i);

            if(ch>='A' && ch<='Z'){

                res.append((char)(ch+32));
            }
            else{
                res.append(ch);
            }
        }
        return res.toString();
    }
}