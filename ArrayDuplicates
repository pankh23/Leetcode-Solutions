class Solution {
    public ArrayList<Integer> findDuplicates(int[] arr) {
        // code here
        ArrayList<Integer> ans=new ArrayList<>();
        HashMap<Integer, Integer> map=new HashMap<>();
        
        for(int i=0;i<arr.length;i++){

            map.put(arr[i], map.getOrDefault(arr[i], 0)+1);
            
            if(map.get(arr[i])>=2){
                ans.add(arr[i]);
            }

        }
        return ans;
    }
}