class Solution {
    static boolean searchKey(int n, Node head, int key) {
        // Code here
        
        if(head==null){
            
            return false;
        }
        
        while(head!=null){
            
            // int v=head.value
            if(head.data==key){
                return true;
            }
            head=head.next;
        }
        return false;
    }
}